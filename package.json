{
  "name": "whisperscribe",
  "version": "0.7.0",
  "description": "Transcribe audio and video files in Obsidian using multiple AI providers",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc main.ts -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "transcription",
    "ai",
    "audio",
    "video",
    "speech-to-text",
    "gemini",
    "openai",
    "whisper"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^16.11.6",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "obsidian": "latest",
    "ts-jest": "^29.4.1",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  }
}
